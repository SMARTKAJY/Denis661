#include<iostream>
int main(){
int age;
std::cout<<"ecrire votre âge:"<<std::endl;
std::cin>>age;
if(age<18){
   std::cout<<"vous êtes mineur"<<std::endl;}else{
    if(age<60){
        std::cout<<"vous êtes adult"<<std::endl;
    }else{
        std::cout<<"vous êtes senior" <<std::endl;
        }
}
    return 0;
}
